 setType(_category.type);\n  };\n\n  const handleBannerImages = (e) => {\n    setBanners([...banners, e.target.files[0]]);\n  };\n\n  const handleProductImages = (e) => {\n    setProducts([...products, e.target.files[0]]);\n  };\n\n  const handleShow = () => setShow(true);\n  const handleClose = () => setShow(false);\n\n  const submitCreatePageForm = (e) => {\n    if (pageName === \"\" || description === \"\" || type===\"\" || categoryId === \"\" ||\n     banners.length===0 || products.length===0) {\n      alert(\"Empty Fields\");\n      return;\n    }\n    const form = new FormData();\n    form.append(\"title\", pageName);\n    form.append(\"category\", categoryId);\n    form.append(\"type\", type);\n    form.append(\"description\", description);\n    banners.forEach((banner, index) => {\n      form.append(\"banners\", banner);\n    });\n    products.forEach((product, index) => {\n      form.append(\"products\", product);\n    });\n    dispatch(createPageAction(form));\n    setShow(false);\n  };\n\n  const renderNewPageModal = () => {\n    return (\n      <Modal\n        modalTitle={\"Create New Page\"}\n        show={show}\n        handleClose={handleClose}\n        onHide={handleClose}\n        action={submitCreatePageForm}\n        _task={\"Create Page\"}\n      >\n        <Container>\n          <Row>\n            <Col>\n              <Input\n                label={\"Page Name\"}\n                value={pageName}\n                placeholder=\"Enter Page Name\"\n                onChange={(e) => setPageName(e.target.value)}\n                className=\"form-control form-control-sm\"\n              />\n            </Col>\n            <Col>\n              <label>Choose a Category</label>\n              {/* <select\n                className=\"form-control form-control-sm\"\n                value={categoryId}\n                onChange={onCategoryChange}\n              >\n                <option>Select Category</option>\n                {createCategoryList(category.categories).map((option) => (\n                  <option key={option.value} value={option.value}>\n                    {option.name}\n                  </option>\n                ))}\n              </select> */}\n\n              <Input\n              type=\"select\"\n              value={categoryId}\n              onChange={onCategoryChange}\n              options={linearCategory}\n              placeholder={\"Choose a Category\"}\n              className={\"form-control form-control-sm\"}\n              />\n              \n            </Col>\n          </Row>\n          <Row>\n            <Col>\n              <Input\n                label={\"Description\"}\n                value={description}\n                placeholder=\"Enter Page Description\"\n                onChange={(e) => setDescription(e.target.value)}\n                className=\"form-control form-control-sm\"\n              />\n            </Col>\n          </Row>\n\n          {banners.length > 0\n            ? banners.map((banner, index) => (\n                <Row key={index}>\n                  <Col>{banner.name}</Col>\n                </Row>\n              ))\n            : null}\n          <Row>\n            <Col>\n              <Input\n                label={\"Banners\"}\n                className=\"form-control form-control-sm\"\n                type=\"file\"\n                name=\"banners\"\n                onChange={handleBannerImages}\n              />\n            </Col>\n          </Row>\n\n          {products.length > 0\n            ? products.map((product, index) => (\n                <Row key={index}>\n                  <Col>{product.name}</Col>\n                </Row>\n              ))\n            : null}\n          <Row>\n            <Col>\n              <Input\n                label={\"Products\"}\n                className=\"form-control form-control-sm\"\n                type=\"file\"\n                name=\"products\"\n                onChange={handleProductImages}\n              />\n            </Col>\n          </Row>\n        </Container>\n      </Modal>\n    );\n  };\n\n  return (\n    <Layout sidebar>\n      {\n          \n          page.loading ? \n          <Container>\n          <p>please wait.......creating New Page</p> \n          </Container> : \n          <>\n          <Container>\n          <Row>\n          <Col>\n            <Button onClick={handleShow}>Create Page</Button>\n          </Col>\n          </Row>\n         </Container>\n      {renderNewPageModal()}\n      </>\n      } \n      </Layout>\n  );\n};\n\nexport default NewPage;\n","/Users/pankajarora/Documents/Web-Development/mern-ecommerce-app/admin-app/src/containers/Orders/index.js",[],"/Users/pankajarora/Documents/Web-Development/mern-ecommerce-app/admin-app/src/containers/Signup/index.js",[],"/Users/pankajarora/Documents/Web-Development/mern-ecommerce-app/admin-app/src/containers/Products/index.js",[],"/Users/pankajarora/Documents/Web-Development/mern-ecommerce-app/admin-app/src/containers/Home/index.js",[],"/Users/pankajarora/Documents/Web-Development/mern-ecommerce-app/admin-app/src/actions/constants.js",[],"/Users/pankajarora/Documents/Web-Development/mern-ecommerce-app/admin-app/src/reducers/auth.reducer.js",[],"/Users/pankajarora/Documents/Web-Development/mern-ecommerce-app/admin-app/src/helpers/axios.js",[],"/Users/pankajarora/Documents/Web-Development/mern-ecommerce-app/admin-app/src/reducers/product.reducer.js",[],"/Users/pankajarora/Documents/Web-Development/mern-ecommerce-app/admin-app/src/reducers/order.reducer.js",[],"/Users/pankajarora/Documents/Web-Development/mern-ecommerce-app/admin-app/src/reducers/page.reducer.js",[],"/Users/pankajarora/Documents/Web-Development/mern-ecommerce-app/admin-app/src/reducers/category.reducer.js",[],"/Users/pankajarora/Documents/Web-Development/mern-ecommerce-app/admin-app/src/reducers/user.reducer.js",[],"/Users/pankajarora/Documents/Web-Development/mern-ecommerce-app/admin-app/src/actions/category.action.js",[],"/Users/p